{
  "tasks": {
    "start": "deno run --allow-read --allow-write --allow-run --allow-net main.ts",
    "build-windows": "deno compile --target x86_64-pc-windows-msvc -o ./builds/buzzlink.exe --allow-read --allow-write --allow-run --allow-net main.ts ",
    "build-mac64": "deno compile --target x86_64-apple-darwin -o ./builds/buzzlink-mac64 --allow-read --allow-write --allow-run --allow-net main.ts ",
    "build-macARM": "deno compile --target aarch64-apple-darwin -o ./builds/buzzlink-macARM --allow-read --allow-write --allow-run --allow-net main.ts ",
    "build-linux64": "deno compile --target x86_64-unknown-linux-gnu -o ./builds/buzzlink-linux64 --allow-read --allow-write --allow-run --allow-net main.ts ",
    "build-linuxARM": "deno compile --target aarch64-unknown-linux-gnu -o ./builds/buzzlink-linuxARM --allow-read --allow-write --allow-run --allow-net main.ts ",
    "build-all": "deno task build-windows && deno task build-mac64 && deno task build-macARM && deno task build-linux64 && deno task build-linuxARM"
